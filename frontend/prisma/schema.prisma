generator client {
  provider = "prisma-client-js"
  output   = "../lib/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Pinata {
  pda        String   @id
  pinataUrl  String
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
}